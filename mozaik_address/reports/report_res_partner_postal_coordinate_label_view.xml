<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="base.res_partner_address_report" model="ir.actions.report.xml">
            <field name="groups_id" eval="[(6, 0, [ref('settings_improvement.unused_features')])]" />
        </record>

        <record id="res_partner_print_label_actions" model="ir.actions.server">
            <field name="name">Print Labels</field>
            <field name="sequence" eval="5"/>
            <field name="state">code</field>
            <field name="type">ir.actions.server</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="code">action = self.get_print_coordinate_actions(cr, uid, context)</field>
            <field name="condition">True</field>
            <field name="groups_id"
                eval="[(6, 0, [ref('mozaik_base.mozaik_res_groups_user')])]" />
        </record>

        <record id="res_partner_print_label_values" model="ir.values">
            <field name="name">Print Labels</field>
            <field name="model">res.partner</field>
            <field name="key">action</field>
            <field name="key2">client_print_multi</field>
            <field name="value" eval="'ir.actions.server,%s' % ref('res_partner_print_label_actions')" />
        </record>

    </data>
</openerp>
