<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Virtual Target -->

        <record id="virtual_target_tree_view" model="ir.ui.view">
            <field name="name">virtual.target.tree</field>
            <field name="model">virtual.target</field>
            <field name="arch" type="xml">
                <tree string="Virtual Target" create="0" delete="0">
                    <field name="partner_id" />
                    <field name="email" />
                    <field name="email_unauthorized" />
                    <field name="postal" />
                    <field name="postal_unauthorized" />
                </tree>
            </field>
        </record>

        <!-- Virtual Partner Involvement -->

        <record id="virtual_partner_involvement_search_view" model="ir.ui.view">
            <field name="name">virtual.partner.involvement.search</field>
            <field name="model">virtual.partner.involvement</field>
            <field name="arch" type="xml">
                <search string="Virtual Partner Involvement">
                    <field name="identifier"/>
                    <field string="Birthdate > ..." name="birth_date" filter_domain="[('birth_date','>',self)]"/>
                    <field string="Birthdate &lt; ..." name="birth_date" filter_domain="[('birth_date','&lt;',self)]"/>

                    <field name="partner_id" domain="[('is_assembly','=',False)]" />
                    <field name="int_instance_id" />
                    <field name="involvement_category_id" />

                    <field name="interests_m2m_ids" string="Interests" widget="many2one"/>
                    <field name="competencies_m2m_ids" string="Competencies &amp; Topics" widget="many2one" />
                    <field name="category_id" string="Tag" widget="many2one" />

                    <filter string="Natural Persons" name="type_person" domain="[('is_company','=',0)]"/>
                    <filter string="Legal Persons" name="type_company" domain="[('is_company','!=',0)]"/>
                    <separator/>
                    <filter string="Male" name="male" domain="[('gender','=','m')]" />
                    <filter string="Female" name="female" domain="[('gender','=','f')]" />
                    <separator />
                    <filter string="French" name="french" domain="[('tongue','=','f')]" />
                    <filter string="German" name="german" domain="[('tongue','=','g')]" />
                    <separator />
                    <filter string="Employee" name="employee" domain="[('employee','!=',0)]" />
                    <filter string="Not Employee" name="not_employee" domain="[('employee','=',0)]" />
                    <separator/>
                    <filter name="p_vip" string="VIP Address" domain="[('postal_vip','=',True)]" groups="ficep_base.ficep_res_groups_vip_reader" />
                    <separator/>
                    <filter name="e_vip" string="VIP Email" domain="[('email_vip','=',True)]" groups="ficep_base.ficep_res_groups_vip_reader" />
                    <separator/>
                    <filter name="p_unauthorized" string="Unauthorized Address" domain="[('postal_unauthorized','=',True)]"/>
                    <separator/>
                    <filter name="e_unauthorized" string="Unauthorized Email" domain="[('email_unauthorized','=',True)]"/>

                    <group expand="0" string="Group By..." name="group_by">
                        <filter name="partner" string="Partner" context="{'group_by':'partner_id'}"/>
                        <filter name="int_instance" string="Internal Instance" context="{'group_by':'int_instance_id'}"/>
                        <filter name="involvement_category" string="Involvement Category" context="{'group_by':'involvement_category_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="virtual_partner_involvement_tree_view" model="ir.ui.view">
            <field name="name">virtual.partner.involvement.tree</field>
            <field name="model">virtual.partner.involvement</field>
            <field name="arch" type="xml">
