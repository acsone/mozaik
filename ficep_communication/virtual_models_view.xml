        <!-- Action -->

        <record id="virtual_partner_candidature_action" model="ir.actions.act_window">
            <field name="name">Partners/Candidatures</field>
            <field name="res_model">virtual.partner.candidature</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="search_view_id" ref="virtual_partner_candidature_search_view"/>
            <field name="view_id" ref="virtual_partner_candidature_tree_view"/>
        </record>

        <!-- Virtual Assembly Instance -->

        <record id="virtual_assembly_instance_search_view" model="ir.ui.view">
            <field name="name">virtual.assembly.instance.search</field>
            <field name="model">virtual.assembly.instance</field>
            <field name="arch" type="xml">
                <search string="Virtual Assembly Instance">
                    <field name="partner_id" domain="[('is_assembly','=',True)]"/>
                    <field name="int_instance_id" />

                    <field name="int_category_assembly_id" string="Internal Category Assembly" widget="many2one"/>
                    <field name="sta_category_assembly_id" string="State Category Assembly" widget="many2one"/>
                    <field name="ext_category_assembly_id" string="External Category Assembly" widget="many2one"/>

                    <field name="sta_power_level_id" string="State Power Level" widget="many2one"/>
                    <field name="int_power_level_id" string="Internal Power Level" widget="many2one"/>

                    <field name="interests_m2m_ids" string="Interests" widget="many2one"/>
                    <field name="competencies_m2m_ids" string="Competencies &amp; Topics" widget="many2one" />
                    <field name="category_id" string="Tag" widget="many2one" />

                    <filter name="sta_mod" string="State Assembly" domain="[('model','=','sta.assembly')]"/>
                    <filter name="int_mod" string="Internal Assembly" domain="[('model','=','int.assembly')]"/>
                    <filter name="ext_mod" string="External Assembly" domain="[('model','=','ext.assembly')]"/>
                    <separator/>
                    <filter name="p_vip" string="VIP Address" domain="[('postal_vip','=',True)]" groups="ficep_base.ficep_res_groups_vip_reader" />
                    <separator/>
                    <filter name="e_vip" string="VIP Email" domain="[('email_vip','=',True)]" groups="ficep_base.ficep_res_groups_vip_reader" />
                    <separator/>
                    <filter name="p_unauthorized" string="Unauthorized Address" domain="[('postal_unauthorized','=',True)]"/>
                    <separator/>
                    <filter name="e_unauthorized" string="Unauthorized Email" domain="[('email_unauthorized','=',True)]"/>

                    <group expand="0" string="Group By..." name="group_by">
                        <filter name="partner" string="Partner" context="{'group_by':'partner_id'}"/>
                        <filter name="int_instance" string="Internal Instance" context="{'group_by':'int_instance_id'}"/>
                        <filter name="int_ass_category" string="Internal Assembly Category" context="{'group_by':'int_category_assembly_id'}"/>
                        <filter name="ext_ass_category" string="External Assembly Category" context="{'group_by':'ext_category_assembly_id'}"/>
                        <filter name="sta_ass_category" string="State Assembly Category" context="{'group_by':'sta_category_assembly_id'}"/>
                        <filter name="int_pl" string="Internal Power Level" context="{'group_by':'int_power_level_id'}"/>
                        <filter name="sta_pl" string="State Power Level" context="{'group_by':'sta_power_level_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="virtual_assembly_instance_tree_view" model="ir.ui.view">
            <field name="name">virtual.assembly.instance.tree</field>
            <field name="model">virtual.assembly.instance</field>
            <field name="arch" type="xml">
                <tree string="Virtual Assembly Instance" create="0" delete="0">
                    <field name="partner_id" />
                    <field name="int_instance_id" />

                    <field name="category" />
                </tree>
            </field>
        </record>

        <!-- Add filter virtual assembly instance -->

        <record id="add_filter_from_invovlement_action" model="ir.actions.act_window">
            <field name="name">Add Filter To Distribution List</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="src_model">virtual.assembly.instance</field>
            <field name="res_model">distribution.list.add.filter</field>
            <field name="multi" eval="1"/>
            <field name="target">new</field>
            <field name="view_id" ref="distribution_list.distribution_list_add_filter" />
            <field name="groups_id" eval="[(6, 0, [ref('ficep_base.ficep_res_groups_user')])]" />
        </record>

        <record id="add_filter_from_instance_values" model="ir.values">
            <field name="name">Add Filter To Distribution List</field>
            <field name="model">virtual.assembly.instance</field>
            <field name="key">action</field>
            <field name="key2">client_action_multi</field>
            <field name="value" eval="'ir.actions.act_window,%s' % ref('add_filter_from_invovlement_action')" />
        </record>

        <!-- Action -->

        <record id="virtual_assembly_instance_action" model="ir.actions.act_window">
            <field name="name">Assemblies/Instances</field>
            <field name="res_model">virtual.assembly.instance</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="search_view_id" ref="virtual_assembly_instance_search_view"/>
            <field name="view_id" ref="virtual_assembly_instance_tree_view"/>
        </record>


        <!-- Virtual Model Menus -->

        <!-- Sub-root menu-->
        <menuitem id="virtual_models_submenu" name="Virtual Models"
                  sequence="20" parent="ficep_base.communication_root_menu" />

        <!-- Virtual Partner Involvement -->
        <menuitem id="virtual_partner_involvement_menu" sequence="10" parent="virtual_models_submenu"
                  action="virtual_partner_involvement_action"/>

        <!-- Virtual Partner Relation -->
        <menuitem id="virtual_partner_relation_menu" sequence="10" parent="virtual_models_submenu"
                  action="virtual_partner_relation_action"/>

        <!-- Virtual Partner Instance -->
        <menuitem id="virtual_partner_instance_menu" sequence="10" parent="virtual_models_submenu"
                  action="virtual_partner_instance_action"/>

        <!-- Virtual Partner Mandate -->
        <menuitem id="virtual_partner_mandate_menu" sequence="10" parent="virtual_models_submenu"
                  action="virtual_partner_mandate_action"/>

        <!-- Virtual Partner Candidature -->
        <menuitem id="virtual_partner_candidature_menu" sequence="10" parent="virtual_models_submenu"
                  action="virtual_partner_candidature_action"/>

        <!-- Virtual Assembly Instance  -->
        <menuitem id="virtual_assembly_instance_menu" sequence="10" parent="virtual_models_submenu"
                  action="virtual_assembly_instance_action"/>

    </data>
</openerp>