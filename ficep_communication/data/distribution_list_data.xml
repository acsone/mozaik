<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
    <data noupdate="0">

        <record id="dll_del_mem_card" model="distribution.list.line">
            <field name="name">Waiting Delivery Member Card</field>
            <field name="src_model_id" ref='ficep_communication.model_virtual_partner_membership'/>
            <field name="company_id" ref='base.main_company'/>
            <field name="domain" eval="['&amp;',
                                        ('membership_state_id.code', '=', 'member'),
                                        ('del_mem_card_date', '=', False)]"/>
        </record>

        <record id="dl_del_mem_card" model="distribution.list">
            <field name="name">MEMCARD</field>
            <field name="company_id" ref='base.main_company'/>
            <field name="to_include_distribution_list_line_ids" eval="[[4,ref('dll_del_mem_card')]]"/>
        </record>

    </data>
</openerp>
