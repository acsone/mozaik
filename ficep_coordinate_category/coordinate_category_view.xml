<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
    <data noupdate="0">

        <record id="phone_coordinate_form" model="ir.ui.view">
            <field name="name">coordinate.category.form</field>
            <field name="model">coordinate.category</field>
            <field name="arch" type="xml">
                <form string="Phone Coordinate" version="7.0">
                    <header>
                        <button name="phone_coordinate_action"
                            string="Phone Coordinate" type="object" />
                    </header>
                    <sheet>
                        <field name="name" />
                    </sheet>
                </form>
            </field>
        </record>

        <record id="coordinate_category_action" model="ir.actions.act_window">
            <field name="name">Coordinate Category</field>
            <field name="res_model">coordinate.category</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Configuration" id="configuration_menu"
            parent="ficep_person.person_menu_root" sequence="99" />

        <menuitem name="Coordinate Category" id="coordinate_category_submenu"
            parent="configuration_menu" sequence="99"
            action="coordinate_category_action" />

    </data>
</openerp>