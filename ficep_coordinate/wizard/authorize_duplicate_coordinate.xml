<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
    <data noupdate="0">

        <record id="authorize_duplicate_wizard" model="ir.ui.view">
            <field name="name">authorize.duplicate.coordinate.wizard</field>
            <field name="model">authorize.duplicate.coordinate</field>
            <field name="arch" type="xml">
                <form string="Authorize Duplicate Coordinate" version="7.0">
                    <strong>
                    Some text
                    </strong>
                    <footer>
                        <button string="Validate" name="authorize_duplicate_coordinate"
                                type="object" class="oe_highlight" />
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="authorize_duplicate_coordinate_action" model="ir.actions.act_window">
            <field name="name">Authorize Duplicate Coordinate</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="src_model">res.partner</field>
            <field name="res_model">authorize.duplicate.coordinate</field>
            <field name="target">new</field>
            <field name="view_id" ref="authorize_duplicate_wizard"/>
            <field name="groups_id" eval="[(4, ref('ficep_base.ficep_res_groups_user')),]" />
        </record>

        <record id="authorize_duplicate_value" model="ir.values">
            <field name="name">Authorize Duplicate Coordinate</field>
            <field name="model">res.partner</field>
            <field name="key">action</field>
            <field name="key2">client_action_multi</field>
            <field name="value" eval="'ir.actions.act_window,%s' % ref('authorize_duplicate_coordinate_action')"/>
        </record>

    </data>
</openerp>