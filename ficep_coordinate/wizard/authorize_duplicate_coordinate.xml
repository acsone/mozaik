<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
    <data noupdate="0">

        <record id="authorize_duplicate_coordinate_wizard" model="ir.ui.view">
            <field name="name">authorize.duplicate.coordinate.wizard</field>
            <field name="model">authorize.duplicate.coordinate</field>
            <field name="arch" type="xml">
                <form string="Authorize Duplicate Coordinate" version="7.0">
                    <strong>
                    Selected coordinates are not duplicates. Accept anyway?
                    </strong>
                    <footer>
                        <span name="go-wizard">
                            <button string="Ok" name="button_authorize_duplicate_coordinate"
                                    type="object" class="oe_highlight" />
                            or
                        </span>
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>