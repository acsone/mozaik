<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
    <data noupdate="0">

        <!-- Addresses -->

        <record id="address_1" model="address.address">
            <field name="country_id" ref="base.be"/>
            <field name="address_local_zip_id" ref="local_zip_1"/>
            <field name="address_local_street_id" ref="local_street_1"/>
            <field name="number">5</field>
        </record>

        <record id="address_2" model="address.address">
            <field name="country_id" ref="base.be"/>
            <field name="address_local_zip_id" ref="local_zip_2"/>
            <field name="address_local_street_id" ref="local_street_2"/>
            <field name="number">6</field>
            <field name="box">2B</field>
        </record>

        <record id="address_3" model="address.address">
            <field name="country_id" ref="base.us"/>
            <field name="zip_man">20301</field>
            <field name="town_man">Washington DC</field>
            <field name="street_man">White House</field>
        </record>

        <!-- Postal Coordinates -->

        <record id="postal_coordinate_1" model="postal.coordinate">
            <field name="address_id" ref="address_1"/>
            <field name="partner_id" ref="res_partner_thierry"/>
            <field name="is_main" eval="True" />
        </record>

        <record id="postal_coordinate_2" model="postal.coordinate">
            <field name="address_id" ref="address_2"/>
            <field name="partner_id" ref="res_partner_jacques"/>
            <field name="is_main" eval="True" />
        </record>

        <record id="postal_coordinate_2_duplicate" model="postal.coordinate">
            <field name="address_id" ref="address_2"/>
            <field name="partner_id" ref="res_partner_marc"/>
            <field name="is_main" eval="True" />
            <field name="unauthorized" eval="True" />
        </record>

        <record id="postal_coordinate_3" model="postal.coordinate">
            <field name="address_id" ref="address_3"/>
            <field name="partner_id" ref="res_partner_pauline"/>
            <field name="is_main" eval="True" />
            <field name="vip" eval="True" />
        </record>

        <!-- Co Residency -->

        <record id="co_residency_id_1" model="co.residency">
            <field name="name">Familly Marsh</field>
        </record>

    </data>
</openerp>
