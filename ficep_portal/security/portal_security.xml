<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- do not allow to modify a confirmed membership_request -->
        <record id="membership_request_portal_cud_rule" model="ir.rule">
            <field name="name">Membership Request (CUD) for Portal User</field>
            <field name="model_id" ref="ficep_membership.model_membership_request" />
            <field name="domain_force">[('state','!=', 'draft')]</field>
            <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
            <field name="perm_read" eval="1"/><field name="perm_write" eval="0"/>
            <field name="perm_create" eval="1"/><field name="perm_unlink" eval="0"/>
        </record>

    </data>
</openerp>